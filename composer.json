{
    "name": "soap/bs-one-ui",
    "description": "Laravel Bootstrap admin template",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Soap\\BsOneUi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/filesystem": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0",
        "illuminate/validation": "^9.21|^10.0"
    },
    "conflict": {
        "laravel/framework": "<9.37.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Soap\\BsOneUi\\BsOneUiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
